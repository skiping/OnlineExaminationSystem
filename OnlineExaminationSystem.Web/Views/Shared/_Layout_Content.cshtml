@using Microsoft.Extensions.Localization
@inject IStringLocalizer<ShareResources> ShareLocalizer

@{
    Layout = null;
}
<!DOCTYPE html>
<html>


<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ShareLocalizer["Online Examination"]</title>
    <environment names="Development">
        <link href="~/css/bootstrap.css" rel="stylesheet" />
        <link href="~/css/font-awesome.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />
        <link href="~/css/vue-component.min.css" rel="stylesheet" />
        <link rel="stylesheet" href="~/css/vue-quill-editor/quill.core.css" />
        <link rel="stylesheet" href="~/css/vue-quill-editor/quill.snow.css" />
        <link rel="stylesheet" href="~/css/vue-quill-editor/quill.bubble.css" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/css/content.min.css" asp-append-version="true" />
    </environment>
    @RenderSection("styles", false)

    <script src="~/js/jquery.js"></script>
    <script src="~/js/bootstrap.js"></script>
    <script src="~/js/vue.min.js"></script>
    <script src="~/js/vue-component.min.js"></script>
    <script src="~/js/base.js"></script>
    <script src="~/js/vue-quill-editor/quill.js"></script>
    <script src="~/js/vue-filter.js"></script>
    <script src="~/js/moment.js"></script>
    <script src="~/js/vue-quill-editor/vue-quill-editor.js"></script>
</head>

<body class="gray-bg">
    @RenderBody()

    @RenderSection("scripts", false)
</body>
</html>

